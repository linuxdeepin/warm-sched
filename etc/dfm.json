{
	"Id": "dde-file-manager",
	"TryFile": "/usr/bin/dde-file-manager",
	"Apply": {
		"After": [
			"snapshot:DE"
		]
	},
	"Capture": {
		"After": [
			"inner:user",
			"x11:dde-file-manager"
		],
		"WaitSecond": 5,
		"Lifetime": 10,
		"Method": [
			{
				"Type": "uiapp",
				"WMClass": "dde-file-manager"
			},
			{
				"Type": "filelist",
				"FileList": [
					"/usr/share/dde-file-manager/database/.__deepin.db",
					"/usr/share/dde-file-manager/database/.__main.db",
					"/usr/share/icons",
					"$HOME/.cache/deepin/icons",
					"$HOME/.config/deepin/dde-file-manager",
					"/usr/lib/x86_64-linux-gnu/qt5/plugins/mediaservice/libgstaudiodecoder.so",
					"/usr/lib/x86_64-linux-gnu/qt5/plugins/mediaservice/libgstcamerabin.so",
					"/usr/lib/x86_64-linux-gnu/qt5/plugins/mediaservice/libgstmediacapture.so",
					"/usr/local/share/fonts/.uuid",
					"/usr/share/deepin/dde-file-manager/oem-menuextensions",
					"/usr/share/fontconfig/conf.avail",
					"/usr/share/fonts/cmap/.uuid",
					"/usr/share/fonts/cMap/.uuid",
					"/usr/share/fonts/fonts-cesi/.uuid",
					"/usr/share/fonts/opentype/noto/.uuid",
					"/usr/share/fonts/opentype/.uuid",
					"/usr/share/fonts/truetype/ancient-scripts/.uuid",
					"/usr/share/fonts/truetype/deepin/.uuid",
					"/usr/share/fonts/truetype/dejavu/.uuid",
					"/usr/share/fonts/truetype/droid/.uuid",
					"/usr/share/fonts/truetype/lohit-devanagari/.uuid",
					"/usr/share/fonts/truetype/noto/.uuid",
					"/usr/share/fonts/truetype/unifont/.uuid",
					"/usr/share/fonts/truetype/.uuid",
					"/usr/share/fonts/.uuid",
					"/usr/share/fonts/wps-office/.uuid",
					"/usr/share/fonts/X11/100dpi/.uuid",
					"/usr/share/fonts/X11/75dpi/.uuid",
					"/usr/share/fonts/X11/encodings/large/.uuid",
					"/usr/share/fonts/X11/encodings/.uuid",
					"/usr/share/fonts/X11/misc/.uuid",
					"/usr/share/fonts/X11/Type1/.uuid",
					"/usr/share/fonts/X11/util/.uuid",
					"/usr/share/fonts/X11/.uuid",
					"/usr/share/poppler/cMap/Adobe-CNS1/.uuid",
					"/usr/share/poppler/cMap/Adobe-GB1/.uuid",
					"/usr/share/poppler/cMap/Adobe-Japan1/.uuid",
					"/usr/share/poppler/cMap/Adobe-Japan2/.uuid",
					"/usr/share/poppler/cMap/Adobe-Korea1/.uuid"
				],
				"IncludeList": [
					"/var/lib/dpkg/info/dde-file-manager.list",
					"/var/lib/dpkg/info/dde-desktop.list",
					"/var/lib/dpkg/info/dde-disk-mount-plugin.list",
					"/var/lib/dpkg/info/libdde-file-manager:amd64.list",
					"/var/lib/dpkg/info/libdfm-extension.list"
				]
			}
		]
	}
}
